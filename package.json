{
  "name": "vue-touch-range-integer",
  "version": "1.0.2",
  "description": "Touch extension of input-range element",
  "main": "dist/vue-touch-range.cjs.js",
  "scripts": {
    "dev": "poi",
    "build": "npm run rimraf && bili",
    "build:docs": "npm run rimraf && poi build",
    "pre-prod": "npm run test && npm run build",
    "rimraf": "rimraf dist",
    "lint": "eslint --ext .js,.vue src docs",
    "publish": "npm run build && npm publish --access=public",
    "test": "npm run lint && tyu"
  },
  "poi": {
    "entry": "docs/index.js",
    "dist": "dist",
    "homepage": "./"
  },
  "bili": {
    "format": [
      "cjs",
      "umd"
    ],
    "name": "vue-touch-range",
    "plugins": [
      "vue"
    ]
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bionicmaster/vue-touch-range.git"
  },
  "keywords": [
    "vue",
    "touch",
    "range",
    "slider"
  ],
  "author": "Masaya Kazama <miyan@t-p.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bionicmaster/vue-touch-range/issues"
  },
  "homepage": "https://github.com/bionicmaster/vue-touch-range#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.10",
    "bili": "^1.6.5",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.0",
    "poi": "^9.6.12",
    "rimraf": "^2.6.2",
    "rollup-plugin-vue": "^3.0.0",
    "tyu": "^1.0.5"
  }
}
